<?xml version="1.0" encoding="utf-8"?>
<AxFormExtension xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
	<Name>ProjInvoiceProposalListPage_ActivityExtension</Name>
	<TargetElement>ProjInvoiceProposalListPage</TargetElement>
	<FormDesign>
		<Pattern>ListPage</Pattern>
		<Controls>
			<!-- Add ActivityNumber column to grid -->
			<AxFormGridControl>
				<Name>Grid</Name>
				<FormControlExtension>
					<Controls>
						<AxFormStringControl>
							<Name>ActivityNumber</Name>
							<DataField>ActivityNumber</DataField>
							<DataSource>ProjProposalJour</DataSource>
							<DisplayLength>15</DisplayLength>
						</AxFormStringControl>
					</Controls>
				</FormControlExtension>
			</AxFormGridControl>
		</Controls>
	</FormDesign>
</AxFormExtension>
